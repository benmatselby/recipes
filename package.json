{
  "name": "recipes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "fix": "concurrently npm:fix:*",
    "fix:next": "next lint --fix",
    "fix:prettier": "prettier --write \"{pages,src}/**/*.{js,css}\"",
    "lint": "concurrently npm:lint:*",
    "lint:next": "next lint",
    "lint:prettier": "prettier --check \"{pages,src}/**/*.{js,css}\"",
    "start": "next start",
    "prepare": "husky install",
    "test": "jest --coverage"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "next": "^12.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark": "^14.0.1",
    "remark-html": "^15.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.0",
    "concurrently": "^7.2.2",
    "eslint": "^8.19.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.4.7",
    "prettier": "2.7.1"
  }
}
